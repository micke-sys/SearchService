{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "Debug" }
    ]
  },
  "Swagger": {
    "Endpoint": "/swagger/v1/swagger.json",
    "Name": "My API V1"
  },
  "Development": {
    "UseSwagger": true,
    "UseSwaggerUI": true
  },
  "DataFile": "Data/data.json"
}